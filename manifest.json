{
    "manifest_version": 2,

    "name": "Slithery",
    "description": "Adds extra features to slither.io",
    "version": "0.1.2",

    "icons": {},

    "browser_action": {
        "default_title": "Slithery"
    },

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://slither.io/*"
    ],

    "content_scripts": [{
        "matches": ["http://slither.io/"],
        "js": ["js/injected.js"]
    }],
    "background": {
        "scripts": ["js/background.js"]
    },
    "web_accessible_resources": ["js/game_modified.js"]
}
