{
    "manifest_version": 2,

    "name": "Slithery",
    "description": "Adds extra features to slither.io",
    "version": "0.0.1",

    "icons": {},

    "browser_action": {
        "default_title": "Slithery"
    },

    "content_scripts": [{
        "matches": ["http://slither.io/"],
        "js": ["injected/main.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    }
}
